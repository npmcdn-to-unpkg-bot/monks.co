---
layout: nil
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      {% for hash in post %}
        "hash": "{{ hash }}",
        "hash[0]": "{{ hash[0] }}",
        "hash[1]": "{{ hash[1] }}",
      {% endfor %}
    }
    {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]

