---
layout: nil
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "tags": "{{ post.tags }}",
      {% for hash in post %}
        "hash": "{{ hash }}",
        {% for tag in hash %}
          "tag": "{{ tag }}",
          "{{ tag[0] }}": "{{ tag[1] }}"
          {% if forloop.last %}{% else %},{% endif %}
        {% endfor %}
        "hash.tags": "{{ hash.tags }}",
        "{{ hash[0] }}": "{{ hash[1] }}"
        {% if forloop.last %}{% else %},{% endif %}
      {% endfor %}
    }
    {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]

