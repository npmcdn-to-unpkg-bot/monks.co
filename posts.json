[
  {% for post in site.posts %}
    {
      "tags": "{{ post.tags }}",
      {% for hash in post.tags %}
        "hash": "{{ hash }}",
        "{{ hash[0] }}": "{{ hash[1] }}"
        {% if forloop.last %}{% else %},{% endif %}
      {% endfor %}
    }
    {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]

