---
layout: nil
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      {% for hash in post %}
        "hash": "{{ hash }}",
        {% for tag in hash %}
          "tag": "{{ tag }}",
          "{{ tag[0] }}": "{{ tag[1] }}"
          "{{ hash[0] }}": "{{ hash[1] }}"
        {% endfor %}
        {% if forloop.last %}{% else %},{% endif %}
      {% endfor %}
    }
    {% if forloop.last %}{% else %},{% endif %}
  {% endfor %}
]

